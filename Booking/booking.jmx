            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Define the file path where CSV will be saved

def filePath = &quot;dmoney_user_data.csv&quot;

// Extract the variables from the JSON Extractor

def name = vars.get(&quot;firstname&quot;)

def email = vars.get(&quot;lastname&quot;)

// Log extracted values for debugging

log.info(&quot;Extracted firstname: &quot; + firstname)

log.info(&quot;Extracted lastname: &quot; + lastname)

// Create or append data to the CSV file

def file = new File(filePath)

if (!file.exists()) {

// If file doesn&apos;t exist, write the header

file.append(firstname,lastname\n&quot;)

}
file.append(&quot;${firstname},${lastname}\n&quot;)



// Write the data

file.append(&quot;${name},${email},${phone_number}\n&quot;)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
